<!-- show all questions  -->
<% @home.each do |q| %>
<h3> Title: <%= q.title %></h3>
<p> Question: <br><%= q.questions %></p> <br>
<%= q.created_at %> <br>
<p>Posted by: <%= q.user.email %></p>
<%= link_to "Show", user_question_path(q.user_id, q) %>
<hr>
<% end %>

